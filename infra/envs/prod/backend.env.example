# AI Knowledge Agent Backend - Production Environment Variables
# Copy this file to backend.env and fill in the actual values
# DO NOT commit backend.env to version control

# Application Configuration
APP_NAME=AI Knowledge Agent Backend
APP_VERSION=1.0.0
ENVIRONMENT=production
LOG_LEVEL=INFO

# Database Configuration (non-sensitive)
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30
DB_ECHO=false
DB_ECHO_POOL=false

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# API Configuration
BACKEND_PORT=8001
CORS_ORIGINS=https://your-frontend-domain.com

# Migration Configuration
DB_MIGRATION_DIR=alembic

# Note: Sensitive values like DATABASE_URL, API keys, etc. are stored in AWS Secrets Manager
# and injected into the ECS task at runtime via the task definition.
